from sklearn import datasets
import matplotlib.pyplot as plt
wine_df=datasets.load_wine()
print(dir(wine_df))
print(wine_df.feature_names)
print(wine_df.target)
print(wine_df.target_names)
label = {0:'class_0',1:'class_1',2:'class_2'}
x_axis = wine_df.data[:,0]
y_axis = wine_df.data[:,1]
plt.scatter(x_axis,y_axis,c=wine_df.target)
plt.show()